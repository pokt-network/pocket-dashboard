    "big-integer": "^1.6.48",
    "bull": "^3.14.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.16.1",
    "express-jwt": "^6.0.0",
    "googleapis": "^61.0.0",
    "husky": "^4.3.0",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.5.4",
    "morgan": "^1.9.1",
    "node-cron": "^2.0.3",
